        <mxCell id="2fLScxl3ieeY5KlzS-4u-4" value="&lt;h2&gt;A single microservice contains&lt;/h2&gt;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="2fLScxl3ieeY5KlzS-4u-60">
          <mxGeometry x="152.5" y="10" width="305" height="30" as="geometry" />
